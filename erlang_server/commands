// show content of tables
    rp(ets:tab2list(user)).
    rp(ets:tab2list(trip)).

// start main_server and loop_server
     loop_server:init_listener().

// create new trip
    loop_server ! {self(), create_trip, {matteo, 'gita', 'Pisa', '10/02/2023', 10}}.

// clear table
    loop_server ! {self(), reset_trips}.

// start from supervisor
    erlang_server_sup:start_link().

// start from main
    erlang_server_app:start_main_server().